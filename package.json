{
  "name": "disucss",
  "version": "1.0.0",
  "description": "A react chat app",
  "scripts": {
    "start": "ts-node src/server/src/server.ts",
    "type-check": "cd src/client && tsc --noEmit",
    "type-check:watch": "npm run type-check -- --watch",
    "build": "cd src/client && webpack --progress --colors --mode=production",
    "dev": "cd src/client && webpack-dev-server"
  },
  "author": {
    "name": "Lightning"
  },
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "7.4.4",
    "@babel/plugin-proposal-class-properties": "7.4.4",
    "@babel/plugin-proposal-object-rest-spread": "7.4.4",
    "@babel/preset-env": "7.4.4",
    "@babel/preset-react": "7.0.0",
    "@babel/preset-typescript": "7.3.3",
    "babel-loader": "8.0.6",
    "fork-ts-checker-webpack-plugin": "1.3.3",
    "html-webpack-plugin": "3.2.0",
    "typescript": "3.4.5",
    "webpack": "4.31.0",
    "webpack-cli": "3.3.2",
    "webpack-dev-server": "3.4.1",
    "@types/lodash": "^4.14.119",
    "@babel/plugin-proposal-partial-application": "^7.4.0",
    "@babel/plugin-proposal-decorators": "^7.2.0",
    "@babel/plugin-proposal-optional-chaining": "^7.2.0",
    "@babel/plugin-proposal-do-expressions": "^7.5.0",
    "file-loader": "^3.0.1",
    "css-loader": "^2.1.1",
    "style-loader": "^0.23.1",
    "html-loader": "^0.5.5",
    "@types/react": "^16.7.17",
    "@types/react-dom": "^16.0.11",
    "ts-node": "^8.3.0"
  },
  "dependencies": {
    "express": "^4.16.4",
    "lodash": "^4.17.11",
    "react": "16.8.6",
    "react-dom": "16.8.6",
    "socket.io": "^2.5.0"
  },
  "resolutions": {
    "terser": "3.14.1"
  }
}
